import org.meta.molecule.*;
import org.meta.fragment.*;

MolecularFormula molecularformula(Molecule molecule) {
    return new MolecularFormula(molecule);
}

MolecularFormula molecularformula(Fragment fragment) {
    return new MolecularFormula(fragment);
}
