<propertysheet title="Textured plane renderer settings">
    <propertygroup name="General">
      <property name="texturePlane"
                displayName="Texture Plane :"
                mnemonicChar="T"
                tooltipText="The texture plane to be displayed"
                get="true"
                set="true"
                defaultUI="true" /> 
      <property name="displayPlane"
                displayName="Display Plane :"
                mnemonicChar="D"
                tooltipText="The plane number to be displayed"
                get="true"
                set="true"
                defaultUI="false" />   
    </propertygroup>
</propertysheet>    
                